install-dist_referenceDATA: $(dist_reference_DATA)
	@$(NORMAL_INSTALL)
	test -z "$(referencedir)" || $(MKDIR_P) "$(DESTDIR)$(referencedir)"
	@list='$(dist_reference_DATA)'; test -n "$(referencedir)" || list=; \
	for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  echo "$$d$$p"; \
	done | $(am__base_list) | \
	while read files; do \
	  echo " $(INSTALL_DATA) $$files '$(DESTDIR)$(referencedir)'"; \
	  $(INSTALL_DATA) $$files "$(DESTDIR)$(referencedir)" || exit $$?; \
	done

uninstall-dist_referenceDATA:
	@$(NORMAL_UNINSTALL)
	@list='$(dist_reference_DATA)'; test -n "$(referencedir)" || list=; \
	files=`for p in $$list; do echo $$p; done | sed -e 's|^.*/||'`; \
	test -n "$$files" || exit 0; \
	echo " ( cd '$(DESTDIR)$(referencedir)' && rm -f" $$files ")"; \
	cd "$(DESTDIR)$(referencedir)" && rm -f $$files
