+
+class EchoStructTest : public XmlRpcServerMethod
+{
+public:
+  EchoStructTest(XmlRpcServer* s) : XmlRpcServerMethod("validator1.echoStructTest", s) {}
+
+  void execute(XmlRpcValue& params, XmlRpcValue& result)
+  {
+    std::cerr << "EchoStructTest\n";
+    result = params[0];
+  }
+} echoStructTest(&s);
+
