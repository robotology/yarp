+int main(int argc, char* argv[])
+{
+  if (argc != 2) {
+    std::cerr << "Usage: Validator port\n";
+    return -1;
+  }
+  int port = atoi(argv[1]);
+
+  XmlRpc::setVerbosity(5);
+
+  // Create the server socket on the specified port
+  s.bindAndListen(port);
+
+  // Wait for requests indefinitely
+  s.work(-1.0);
+
+  return 0;
